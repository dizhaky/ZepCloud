# RAG-Anything M365 Integration Configuration
# Copy this file to .env and fill in your credentials

# OpenAI API (required for RAG-Anything processing)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1  # Optional, for custom endpoints

# Azure AI Search (from existing setup)
AZURE_SEARCH_SERVICE_NAME=typingmind-search-danizhaky
AZURE_SEARCH_ADMIN_KEY=your_admin_key_here
AZURE_SEARCH_ENDPOINT=https://typingmind-search-danizhaky.search.windows.net
AZURE_SEARCH_INDEX_NAME=azureblob-index

# Azure Storage (from existing setup)
AZURE_STORAGE_ACCOUNT_NAME=tmstorage0731039
AZURE_STORAGE_ACCOUNT_KEY=your_storage_key_here
AZURE_STORAGE_CONTAINER_NAME=training-data

# M365 Authentication (from existing setup)
M365_CLIENT_ID=your_m365_client_id
M365_CLIENT_SECRET=your_m365_client_secret
M365_TENANT_ID=your_m365_tenant_id

# RAG-Anything Configuration
RAG_ANYTHING_PARSER=auto  # auto, mineru, or docling
RAG_ANYTHING_PARSE_METHOD=auto  # auto, ocr, or txt
RAG_ANYTHING_OUTPUT_DIR=./enhanced_output
RAG_ANYTHING_BATCH_SIZE=25

# Processing Configuration
ENABLE_TABLE_EXTRACTION=true
ENABLE_EQUATION_EXTRACTION=true
ENABLE_IMAGE_PROCESSING=true
ENABLE_GRAPH_RELATIONSHIPS=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/raganything_processor.log
