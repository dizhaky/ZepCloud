{
  "mcpServers": {
    "byterover": {
      "command": "npx",
      "args": ["@byterover/mcp-server"],
      "env": {
        "BYTEROVER_API_KEY": "YOUR_BYTEROVER_API_KEY_HERE"
      }
    },
    "task-master-ai": {
      "command": "npx",
      "args": ["@task-master-ai/mcp-server"],
      "env": {
        "TASK_MASTER_API_KEY": "YOUR_TASK_MASTER_API_KEY_HERE"
      }
    }
  }
}
